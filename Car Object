class Car
{
  TripComputer trip_comp;
  FuelTank fuel_tank;
  FuelComputer fuel_comp;
  
  Car ()
  {
    trip_comp= new TripComputer();
    fuel_tank= new FuelTank();
    fuel_comp = new FuelComputer();
  }
  
  void processInput (SensorDataProvider sdp1)
  {
   trip_comp.setTripComputerVariables(sdp1.readRPM(), sdp1.readRatio());
   fuel_tank.setFuelTank(sdp1.readFuelLevel());
   fuel_comp.setFuelComputer(trip_comp.totalTravelledDistance);
  }
}
